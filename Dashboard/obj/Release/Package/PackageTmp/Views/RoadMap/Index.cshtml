        <div class="modal fade hmodal-danger" id="@myModalCampaign.ID" tabindex="-1" role="dialog" aria-hidden="true" style="display: none;">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="color-line"></div>
                    <div class="modal-header">
                        <h4 class="modal-title">@myModalCampaign.Name</h4>
                        <h5>Objective: @myModalCampaign.Objective</h5>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-lg-12">
                                <dl class="dl-horizontal">
                                    <dt style="padding: 5px;">Campaign Plan</dt>
                                    <dd style="padding: 5px;">@myModalCampaign.CampaignPlan.PlanName</dd>
                                    <dt style="padding: 5px;">Campaign Type</dt>
                                    <dd style="padding: 5px;">@myModalCampaign.CampaignType.Type</dd>
                                    <dt style="padding: 5px;">Campaign Lead</dt>
                                    <dd style="padding: 5px;">@myModalCampaign.CampaignLead.FirstName @myModalCampaign.CampaignLead.LastName</dd>
                                    <dt style="padding: 5px;">Status</dt>
                                    <dd style="padding: 5px;">@myModalCampaign.Status.Name</dd>
                                    <dt style="padding: 5px;">Email System</dt>
                                    <dd style="padding: 5px;">@myModalCampaign.EmailSystem.Name</dd>
                                    <dt style="padding: 5px;">Trades Included</dt>
                                    <dd style="padding: 5px;">
                                        @{
                                            foreach (var recordDetailse in ownerDetails.Where(p => p.CampaignId == myModalCampaign.ID))
                                            {
                                                <span class="badge" style="background-color: @recordDetailse.Color;">@recordDetailse.TradeName</span>
                                            }
                                        }
                                    </dd>
                                </dl>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="table-responsive">
                                    <table cellpadding="1" cellspacing="1" class="table table-condensed table-striped">
                                        <thead>
                                        <tr>
                                            <th>Trade</th>
                                            <th>Campaign Owner</th>
                                            <th>Role</th>
                                            <th>Email</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        @{
                                            foreach (var data in ownerDetails.Where(p => p.CampaignId == myModalCampaign.ID))
                                            {
                                                <tr>
                                                    <td>@data.TradeName</td>
                                                    <td>@data.Name</td>
                                                    <td>@data.Role</td>
                                                    <td>@data.Email</td>
                                                </tr>
                                            }
                                        }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn w-xs btn-danger" onclick="window.open('/Campaigns/Edit/@myModalCampaign.ID');" style="color: white;float:left;">
                            <i class="pe-7s-note"></i>
                            Campaign Notes
                        </button>
                        <button type="button" class="btn w-xs btn-danger" onclick="location.href = '/Campaigns/Details/@myModalCampaign.ID';" style="color: white;">
                            View Campaign Details
                        </button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                        </div>
                    </div>
                </div>
            }
        }


