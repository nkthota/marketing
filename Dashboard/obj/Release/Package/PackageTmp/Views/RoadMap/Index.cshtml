@model IEnumerable<Dashboard.Models.Campaign>

@{
    ViewBag.Title = "RoadMap";
    var currentRowData = "";
    List<CalenderYear> displayCalender = ViewBag.CalenderYear;
    List<Dashboard.Models.Trade> currentTrades = ViewBag.Trades;
    List<TradeDetails> ownerDetails = ViewBag.Owners;
    List<Dashboard.Models.CampaignType> campaignTypes = ViewBag.CampaignTypes;
    ScheduleFilter filters = ViewBag.ScheduleFilters;
    var panelBodyColor = "#E8E5E0";
}

<div class="normalheader ">
    <div class="hpanel">
        <div class="panel-body" style="background-color: #F3F0EB; border: 0;">
            
            <div class="row" style="margin-top: -20px;">
                <div class="col-lg-5">
                    <div id="hbreadcrumb" class="pull-left m-t-lg">
                        <ol class="hbreadcrumb breadcrumb" style="background-color: #F3F0EB;">
                            <li><a href="/RoadMap">Roadmap</a></li>
                            <li>
                                <span>@filters.TradeName</span>
                            </li>
                            <li>
                                <span>@filters.CampaignType</span>
                            </li>
                        </ol>
                    </div>
                </div>
                
                <div class="col-lg-3">
                    <div class="pull-left m-t-lg">
                    </div>
                </div>
                
                <div class="col-lg-4">
                    <div class="pull-right m-t-lg">
                        <div class="btn-group">

                            <button data-toggle="dropdown" class="btn btn-outline btn-danger dropdown-toggle" aria-expanded="false"><i class="pe-7s-filter" style="padding-right: 10px;"></i>CAMPAIGN TYPE <span class="caret"></span></button>
                            <ul class="dropdown-menu">
                                @foreach (var campaignType in campaignTypes)
                                {
                                    if (ViewContext.RouteData.Values.ContainsKey("id"))
                                    {
                                        <li>
                                            <a href="/RoadMap/Index/@ViewContext.RouteData.Values["id"].ToString()/@campaignType.ID">@campaignType.Type</a>
                                        </li>
                                    }
                                    else
                                    {
                                        <li>
                                            <a href="/RoadMap/Index/0/@campaignType.ID">@campaignType.Type</a>
                                        </li>
                                    }
                                }
                                <li class="divider"></li>
                                @if (ViewContext.RouteData.Values.ContainsKey("id") && @ViewContext.RouteData.Values["id"].ToString() != "0")
                                {
                                    <li>
                                        <a href="/RoadMap/Index/@ViewContext.RouteData.Values["id"].ToString()">All Campaign Types</a>
                                    </li>
                                }
                                else
                                {
                                    <li>
                                        <a href="/RoadMap/Index">All Campaign Types</a>
                                    </li>
                                }

                            </ul>
                        </div>
                        <div class="btn-group">

                            <button data-toggle="dropdown" class="btn btn-outline btn-danger dropdown-toggle" aria-expanded="false"><i class="pe-7s-filter" style="padding-right: 10px;"></i>VIEW BY TRADE <span class="caret"></span></button>
                            <ul class="dropdown-menu">
                                @foreach (var trade in currentTrades)
                                {
                                    if (ViewContext.RouteData.Values.ContainsKey("type"))
                                    {
                                        <li>
                                            <a href="/RoadMap/Index/@trade.ID/@ViewContext.RouteData.Values["type"].ToString()">@trade.Name</a>
                                        </li>
                                    }
                                    else
                                    {
                                        <li>
                                            <a href="/RoadMap/Index/@trade.ID">@trade.Name</a>
                                        </li>
                                    }
                                }
                                <li class="divider"></li>
                                <li>
                                    <a href="/RoadMap/Index">All Trades</a>
                                </li>
                            </ul>
                        </div>
                    </div>

                </div>

            </div>


        </div>
    </div>
</div>
<div class="content">
@{
